<%include ../partials/header%>
<div class="container">
    <div class="row">
        <h1 class="display-3">Classroom for this semester</h1>
    </div>
    <div class="row">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">Subject ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Room</th>
                    <th scope="col">Schedule</th>
                </tr>
            </thead>
            <tbody>
                <% classes.forEach(function(cls){ %>
                <tr>
                    <th scope="row">
                    <a href="classdetail/<%= cls.subject_id %>">
                        <%= cls.subject_id %>
                    </a>
                    </th>
                    <td><%= cls.subject_name %></td>
                    <td><%= cls.room %></td>
                    <td><%= cls.schedule%></td>
                </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

</div>
<%include ../partials/footer%>